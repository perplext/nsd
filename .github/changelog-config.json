{
  "categories": [
    {
      "title": "## ğŸš€ Features",
      "labels": ["feature", "enhancement"]
    },
    {
      "title": "## ğŸ› Bug Fixes",
      "labels": ["bug", "fix"]
    },
    {
      "title": "## ğŸ”’ Security",
      "labels": ["security"]
    },
    {
      "title": "## ğŸ“š Documentation",
      "labels": ["documentation", "docs"]
    },
    {
      "title": "## ğŸ§ª Testing",
      "labels": ["test", "testing"]
    },
    {
      "title": "## ğŸ—ï¸ Infrastructure",
      "labels": ["ci", "build", "infrastructure"]
    },
    {
      "title": "## â¬†ï¸ Dependencies",
      "labels": ["dependencies", "deps"]
    }
  ],
  "ignore_labels": [
    "ignore-for-release",
    "invalid",
    "wontfix",
    "duplicate"
  ],
  "sort": "ASC",
  "template": "${{CHANGELOG}}\n\n**Full Changelog**: ${{OWNER}}/${{REPO}}/compare/${{FROM_TAG}}...${{TO_TAG}}",
  "pr_template": "- ${{TITLE}} @${{AUTHOR}} (#${{NUMBER}})",
  "empty_template": "No changes",
  "label_extractor": [
    {
      "pattern": "(feat|feature)",
      "label": "feature"
    },
    {
      "pattern": "fix",
      "label": "bug"
    },
    {
      "pattern": "(doc|docs)",
      "label": "documentation"
    }
  ],
  "transformers": [
    {
      "pattern": "\\[.*\\]",
      "flags": "g",
      "target": ""
    }
  ],
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "max_back_track_time_days": 365,
  "exclude_merge_branches": [],
  "tag_resolver": {
    "method": "semver"
  },
  "base_branches": ["main"]
}